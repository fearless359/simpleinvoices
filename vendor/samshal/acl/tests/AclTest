namespace Samshal\Acl\Test;

use Samshal\Acl\Acl;
use Samshal\Acl\Role\DefaultRole as Role;
use Samshal\Acl\Resource\DefaultResource as Resource;
use Samshal\Acl\Permission\DefaultPermission as Permission;

/**
 * class AclTest.
 */
class AclTest extends \PHPUnit_Framework_TestCase
{
   public function testAddObjects()
   {
        $acl = new Acl();
        $acl->add(
            new Role('role1');
            new Resource('rsx1');
            new Permission('permm1');
        );
   }
}
