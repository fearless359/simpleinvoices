  "si_sql_patchmanager": [
    {
      "sql_patch_ref": "319",
      "sql_patch": "Add set_aging field to si_preferences",
      "sql_release": "20200123",
      "sql_statement": "ALTER TABLE `si_preferences` ADD COLUMN `set_aging` BOOL NOT NULL DEFAULT 0 AFTER `index_group`;UPDATE `si_preferences` SET `set_aging` = 1 WHERE pref_id = 1;",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "320",
      "sql_patch": "Remove deleted extensions mini and measurement",
      "sql_release": "20200822",
      "sql_statement": "DELETE IGNORE FROM `si_extensions` WHERE `name` = 'mini' OR `name` = 'measurement';",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "321",
      "sql_patch": "Add parent_customer_id field to the database",
      "sql_release": "20200924",
      "sql_statement": "ALTER TABLE `si_customers` ADD `parent_customer_id` INT(11) NULL AFTER `notes`;DELETE IGNORE FROM `si_extensions` WHERE `name` = 'sub_customer';INSERT INTO `si_system_defaults` (`name`, `value`, `domain_id`, `extension_id`) VALUES ('sub_customer', $flg, $domainId, 1);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "322",
      "sql_patch": "Add product_groups table to the database.",
      "sql_release": "20201010",
      "sql_statement": "CREATE TABLE `si_product_groups` (name VARCHAR(60) NOT NULL PRIMARY KEY, markup INT(2) NOT NULL DEFAULT 0) ENGINE = InnoDb; DELETE IGNORE FROM `si_extensions` WHERE `name` = 'invoice_grouped'; INSERT INTO `si_system_defaults` (`name`, `value`, `domain_id`, `extension_id`) VALUES ('product_groups', 0, $domainId, 1); ALTER TABLE `si_products` ADD product_group VARCHAR(60) NOT NULL DEFAULT ''; INSERT INTO `si_product_groups` (`name`, `markup`) VALUES ('Labor', 0); INSERT INTO `si_product_groups` (`name`, `markup`) VALUES ('Equipment', 0); INSERT INTO `si_product_groups` (`name`, `markup`) VALUES ('Materials', 0); INSERT INTO `si_product_groups` (`name`, `markup`) VALUES ('Subcontractor', 0);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "323",
      "sql_patch": "Add invoice description open option.",
      "sql_release": "20210413",
      "sql_statement": "INSERT INTO `si_system_defaults` (name ,value ,domain_id ,extension_id ) VALUES ('invoice_description_open', 0, $domainId, 1);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "324",
      "sql_patch": "Rename si_products_values table to si_products_attributes_values.",
      "sql_release": "20210527",
      "sql_statement": "ALTER TABLE `si_products_values` RENAME TO si_products_attributes_values;",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "325",
      "sql_patch": "Remove unused items from the si_system_defaults table.",
      "sql_release": "20200615",
      "sql_statement": "DELETE IGNORE FROM `si_system_defaults` WHERE `name` in ('company_name', 'emailhost', 'emailpassword', 'emailusername', 'pdfbottommargin', 'pdfleftmargin', 'pdfpapersize', 'pdfrightmargin', 'pdfscreensize', 'pdftopmargin', 'spreadsheet', 'wordprocessor'); DELETE IGNORE FROM `si_system_defaults` WHERE `name` LIKE 'dateformat%';",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "326",
      "sql_patch": "Add display department option.",
      "sql_release": "20210930",
      "sql_statement": "INSERT INTO `si_system_defaults` (name ,value ,domain_id ,extension_id ) VALUES ('display_department', 1, $domainId, 1);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "327",
      "sql_patch": "Make invoice_item_id a key field in the invoice_item_tax table.",
      "sql_release": "20220909",
      "sql_statement": "ALTER TABLE `si_invoice_item_tax` ADD KEY `invoice_item_id` (`invoice_item_id`);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "328",
      "sql_patch": "Add cron_invoice_items table to the database.",
      "sql_release": "20220909",
      "sql_statement": "CREATE TABLE `si_cron_invoice_items` (id int(11) UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY, `cron_id` int(11) UNSIGNED NOT NULL, `quantity` decimal(25,6) NOT NULL DEFAULT 0.000000, `product_id` int(11) UNSIGNED NOT NULL, `unit_price` decimal(25,6) DEFAULT 0.000000, `tax_amount` decimal(25,6) DEFAULT 0.000000, `gross_total` decimal(25,6) DEFAULT 0.000000, `description` text COLLATE utf8_unicode_ci DEFAULT NULL, `total` decimal(25,6) DEFAULT 0.000000, `attribute` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL) ENGINE = InnoDb DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;ALTER TABLE `si_cron_invoice_items` ADD KEY `cron_id` (`cron_id`), ADD KEY `product_id` (`product_id`);ALTER TABLE `si_cron_invoice_items` ADD CONSTRAINT `si_cron_invoice_items_ibfk_1` FOREIGN KEY (`cron_id`) REFERENCES `si_cron` (`id`) ON UPDATE CASCADE, ADD CONSTRAINT `si_cron_invoice_items_ibfk_2` FOREIGN KEY (`product_id`) REFERENCES `si_products` (`id`) ON UPDATE CASCADE;",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "329",
      "sql_patch": "Add cron_invoice_item_tax table to the database.",
      "sql_release": "20220909",
      "sql_statement": "CREATE TABLE `si_cron_invoice_item_tax` (id int(11) UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY, `cron_invoice_item_id` int(11) UNSIGNED NOT NULL, `tax_id` int(11) UNSIGNED NOT NULL, `tax_type` char(1) COLLATE utf8_unicode_ci DEFAULT NULL, `tax_rate` decimal(25,6) NOT NULL, `tax_amount` decimal(25,6) NOT NULL) ENGINE = InnoDb DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;ALTER TABLE `si_cron_invoice_item_tax` ADD KEY `cron_invoice_item_id` (`cron_invoice_item_id`), ADD KEY `tax_id` (`tax_id`);ALTER TABLE `si_cron_invoice_item_tax` ADD CONSTRAINT `si_cron_invoice_item_tax_ibfk_1` FOREIGN KEY (`tax_id`) REFERENCES `si_tax` (`tax_id`) ON UPDATE CASCADE;ADD CONSTRAINT `si_cron_invoice_item_tax_ibfk_2` FOREIGN KEY (`cron_invoice_item_id`) REFERENCES `si_cron_invoice_items` (`id`) ON UPDATE CASCADE;",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "330",
      "sql_patch": "Add invoice_item_id as a key for the invoice_item_tax table.",
      "sql_release": "20220926",
      "sql_statement": "ALTER TABLE `si_invoice_item_tax` MODIFY `invoice_item_id` INT(11) UNSIGNED NOT NULL; ALTER TABLE `si_invoice_item_tax` MODIFY `tax_id` INT(11) UNSIGNED NOT NULL; ALTER TABLE `si_invoice_item_tax` ADD KEY `invoice_item_id` (`invoice_item_id`);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "331",
      "sql_patch": "Add foreign key for invoice_item_id to invoice_item_tax table.",
      "sql_release": "20221013",
      "sql_statement": "ALTER TABLE `si_invoice_item_tax` ADD CONSTRAINT `si_invoice_item_tax_ibfk_2` FOREIGN KEY (`invoice_item_id`) REFERENCES `si_invoice_items` (`id`) ON UPDATE CASCADE;",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "332",
      "sql_patch": "Add payment_warehouse table to the database.",
      "sql_release": "20221013",
      "sql_statement": "CREATE TABLE `si_payment_warehouse` (id int(11) UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY, `customer_id` int(11) UNSIGNED NOT NULL, `amount` decimal(25,6) NOT NULL, `payment_type` int(11) UNSIGNED NOT NULL, `check_number` varchar(10) DEFAULT NULL) ENGINE = InnoDb DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;ALTER TABLE `si_payment_warehouse` ADD KEY `customer_id` (`customer_id`), ADD KEY `payment_type` (`payment_type`);ALTER TABLE `si_payment_warehouse` ADD CONSTRAINT `si_customer_id_ibfk_1` FOREIGN KEY (`customer_id`) REFERENCES `si_customers` (`id`) ON UPDATE CASCADE,ADD CONSTRAINT `si_payment_type_ibfk_2` FOREIGN KEY (`payment_type`) REFERENCES `si_payment_types` (`pt_id`) ON UPDATE CASCADE;",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "333",
      "sql_patch": "Add warehouse_amount field to payment table.",
      "sql_release": "20221019",
      "sql_statement": "ALTER TABLE `si_payment` ADD `customer_id` int(11) UNSIGNED AFTER `ac_inv_id`, ADD `warehouse_amount` decimal(25,6) NOT NULL DEFAULT 0 AFTER `ac_check_number`; ALTER TABLE `si_payment` ADD KEY `customer_id` (`customer_id`); ALTER TABLE `si_payment` ADD CONSTRAINT `si_payment_ibfk_3` FOREIGN KEY (`customer_id`) REFERENCES `si_customers` (`id`) ON UPDATE SET NULL ON DELETE SET NULL;",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "334",
      "sql_patch": "Add payment delete days option.",
      "sql_release": "20221022",
      "sql_statement": "INSERT INTO `si_system_defaults` (name ,value ,domain_id ,extension_id ) VALUES ('payment_delete_days', 0, 1, 1);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "335",
      "sql_patch": "Add invoice display days option.",
      "sql_release": "20221110",
      "sql_statement": "INSERT INTO `si_system_defaults` (name ,value ,domain_id ,extension_id ) VALUES ('invoice_display_days', 0, 1, 1);",
      "source": "fearless359"
    },
    {
      "sql_patch_ref": "336",
      "sql_patch": "Make id type for si_log table consistent with all other id settings.",
      "sql_release": "20230717",
      "sql_statement": "ALTER TABLE `si_log` MODIFY `id` INT(11) UNSIGNED NOT NULL AUTO_INCREMENT;",
      "source": "fearless359"
    }
